{
  "server": {
    "port": 8080,
    "host": "0.0.0.0",
    "logLevel": "debug"
  },
  "queue": {
    "concurrency": 1
  },
  "leoCli": {
    "threads": 2,
    "enableResourceProfiling": false,
    "resourceProfilingInterval": 5000,
    "network": "testnet",
    "endpoint": "https://api.explorer.provable.com/v1"
  },
  "oracle": {
    "verifyAttestation": true,
    "attestationRequest": {
      "url": "price_feed: {{COIN_NAME}}",
      "requestMethod": "GET",
      "selector": "weightedAvgPrice",
      "responseFormat": "json",
      "encodingOptions": {
        "value": "float",
        "precision": 6
      }
    },
    "verifier": {
      "address": "aleorclvrfy.venture23.xyz",
      "port": 443,
      "https": true,
      "resolve": false,
      "tls": null
    },
    "notarizers": [
      {
        "address": "20.83.185.145",
        "port": 8443,
        "https": true,
        "resolve": false,
        "tls": {
          "certPath": "./certs/notarization/client.crt",
          "keyPath": "./certs/notarization/client.key",
          "caPath": "./certs/notarization/ca.crt",
          "rejectUnauthorized": true,
          "servername": "nginx-mtls-proxy"
        }
      },
      {
        "address": "20.213.211.94",
        "port": 8000,
        "https": true,
        "resolve": false,
        "tls": {
          "certPath": "./certs/notarization/client.crt",
          "keyPath": "./certs/notarization/client.key",
          "caPath": "./certs/notarization/ca.crt",
          "rejectUnauthorized": true,
          "servername": "nginx-mtls-proxy"
        }
      }
    ],
    "aleoProgram": {
      "name": "vlink_oracle_v2.aleo",
      "function": {
        "setUniqueId": "set_unique_id",
        "setSgxData": "set_data_sgx",
        "setPublicKey": "set_key"
      }
    },
    "supportedCoins": [
      "BTC",
      "ETH",
      "ALEO",
      "USDC",
      "USDT"
    ]
  },
  "cron": {
    "tokens": {
      "BTC": {
        "schedule": "0 0 */2 * * *",
        "enabled": true
      },
      "ETH": {
        "schedule": "0 0 */2 * * *",
        "enabled": true
      },
      "ALEO": {
        "schedule": "0 0 */3 * * *",
        "enabled": true
      },
      "USDC": {
        "schedule": "0 0 */3 * * *",
        "enabled": true
      },
      "USDT": {
        "schedule": "0 0 */3 * * *",
        "enabled": true
      }
    }
  },
  "discord": {
    "webhookUrl": "DISCORD_WEBHOOK_URL",
    "enableTransactionAlert": true,
    "enablePriceUpdateAlert": true,
    "enableCronJobAlert": true,
    "enableServiceStatusAlert": true,
    "enableSystemHealthAlert": true,
    "enableErrorAlert": true
  },
  "security": {
    "internalApiKey": "your-secure-internal-api-key-here",
    "requireApiKey": true
  }
}